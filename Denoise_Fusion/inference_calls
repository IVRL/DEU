#DnCNN for gray models:
python test.py --manipulation_mode Joint --ensemble_method F --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode Joint --ensemble_method S --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode Joint --ensemble_method C --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode SM --ensemble_method F --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode SM --ensemble_method S --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode SM --ensemble_method C --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode FM --ensemble_method F --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode FM --ensemble_method S --denoise_net DnCNN --color_mode gray;
python test.py --manipulation_mode FM --ensemble_method C --denoise_net DnCNN --color_mode gray;
